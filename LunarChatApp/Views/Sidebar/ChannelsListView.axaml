<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:local="using:LunarChatApp"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewmodels="using:LunarChatApp.ViewModels"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="LunarChatApp.ChannelsListView"
			 x:DataType="viewmodels:ChannelListViewModel">
	<StackPanel Spacing="4">
		<ItemsRepeater ItemsSource="{Binding ChannelsList}">
		</ItemsRepeater>
	</StackPanel>
</UserControl>
